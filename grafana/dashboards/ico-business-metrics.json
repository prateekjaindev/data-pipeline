      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "postgres-analytics"
          },
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT \n  iw.name as \"ICO Project\",\n  COUNT(DISTINCT r.user_id) as \"Registrations\",\n  COUNT(DISTINCT c.user_id) as \"Conversions\",\n  COALESCE(SUM(c.amount_usd), 0) as \"Revenue (USD)\",\n  ROUND(\n    CASE \n      WHEN COUNT(DISTINCT r.user_id) > 0 \n      THEN (COUNT(DISTINCT c.user_id)::DECIMAL / COUNT(DISTINCT r.user_id)) * 100 \n      ELSE 0 \n    END, 2\n  ) as \"Conversion Rate (%)\"\nFROM processed_data.ico_websites iw\nLEFT JOIN raw_data.user_registrations r ON iw.id = r.ico_website_id\nLEFT JOIN raw_data.conversions c ON r.user_id = c.user_id \n  AND c.conversion_type = 'presale_participate'\nGROUP BY iw.id, iw.name\nORDER BY \"Revenue (USD)\" DESC\nLIMIT 10;",
          "refId": "A"
        }
      ],
      "title": "ICO Performance by Revenue",
      "type": "table"
