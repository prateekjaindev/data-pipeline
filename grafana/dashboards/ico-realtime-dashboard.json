          "rawQuery": true,
          "rawSql": "WITH session_stats AS (\n  SELECT \n    session_id,\n    COUNT(*) as page_views\n  FROM raw_data.clickstream_events \n  WHERE timestamp >= NOW() - INTERVAL '1 hour'\n    AND event_type = 'page_view'\n  GROUP BY session_id\n)\nSELECT \n  ROUND(\n    (COUNT(*) FILTER (WHERE page_views = 1))::DECIMAL / COUNT(*) * 100, 2\n  ) as bounce_rate\nFROM session_stats;",
          "refId": "A"
        }
      ],
      "title": "Bounce Rate (1h)",
      "type": "stat"
